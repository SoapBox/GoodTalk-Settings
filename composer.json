{
    "name": "soapbox/goodtalk-settings",
    "description": "A package to handle settings.",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Hayes",
            "email": "justin@soapboxhq.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/soapbox/eloquent-model-validation"
        }
    ],
    "require": {
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "symfony/cache": "^5.0",
        "jaspaul/eloquent-model-validation": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "~9.0",
        "fakerphp/faker": "^1.13.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SoapBox\\Settings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests/",
            "SoapBox\\Settings\\Database\\Factories\\":"database/factories/"
        }
    }
}
